.pages
    .page-box
        a.first-page(href='/page/1' class= 'none' )
            i.fas.fa-chevron-right.no-margin
            i.fas.fa-chevron-right.no-margin
        a.prvious-page(href=`#` class= 'none' )
            i.fas.fa-chevron-right
        h6(data-num-of-pages=numOfPages data-current = '1') #{new Intl.NumberFormat('fa-IR').format(1)}
        a.next-page(href=`#` class= numOfPages > 1 ? '':'none')
            i.fas.fa-chevron-left
        a.last-page(href=`#` class= numOfPages > 2 ? '':'none')
            i.fas.fa-chevron-left.no-margin
            i.fas.fa-chevron-left.no-margin
    .temp-circles

